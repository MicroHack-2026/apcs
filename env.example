# API Configuration
# Set the base URL for your Spring Boot backend
VITE_API_BASE_URL=http://localhost:8080/api

# Development Mode
# Set to 'false' to use real API, 'true' to use mock data
# Default: 'true' (uses mock data when backend is not available)
VITE_USE_MOCK_DATA=true

# Environment
NODE_ENV=development
